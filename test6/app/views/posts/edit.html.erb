<h1> Edit Post <h1>

<%= form_for @post ,:html => { :multipart => true } do |f| %>
<p>
<%= f.label :title %><br />
<%= f.text_field :title %>
</p>
<p>
<%= f.label :content %><br />
<%= f.text_area :content %>
</p>
<p>
Photo<br/>
<%= f.file_field :photo %>
</p>
<p>
<%= f.submit "update post" %>
</p>
<% end %>

